<?xml version="1.0"?>
<interface>
  <!-- interface-requires gtk+ 2.14 -->
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="histWin">
    <property name="border_width">5</property>
    <property name="title" translatable="yes">Histogram</property>
