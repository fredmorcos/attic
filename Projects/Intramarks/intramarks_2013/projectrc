PROJECT_LIBS=$HOME/Downloads/intramarks_libs

PROJECT_NODE_MODULES=$PROJECT_LIBS/node_modules
echo "Adding $PROJECT_NODE_MODULES to NODE_PATH"
export NODE_PATH=$NODE_PATH:$PROJECT_NODE_MODULES
echo "NODE_PATH is now $NODE_PATH"

PROJECT_NODE_MODULES_BIN=$PROJECT_NODE_MODULES/.bin
echo "Adding $PROJECT_NODE_MODULES_BIN to PATH"
export PATH=$PATH:$PROJECT_NODE_MODULES_BIN
echo "PATH is now $PATH"
